{
  "kind": "singleType",
  "collectionName": "brandings",
  "info": {
    "singularName": "branding",
    "pluralName": "brandings",
    "displayName": "Branding",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "site_title": {
      "type": "string",
      "maxLength": 64,
      "unique": false
    },
    "site_description": {
      "type": "text",
      "maxLength": 255,
      "unique": false
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "primary_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
