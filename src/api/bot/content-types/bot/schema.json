{
  "kind": "collectionType",
  "collectionName": "bots",
  "info": {
    "singularName": "bot",
    "pluralName": "bots",
    "displayName": "Bot",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "json_training": {
      "type": "json"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "model": {
      "type": "enumeration",
      "enum": [
        "gpt-4",
        "gpt-4-0314",
        "gpt-4-32k",
        "gpt-4-32k-0314",
        "gpt-3.5-turbo",
        "text-davinci-003",
        "text-curie-001",
        "text-babbage-001",
        "text-ada-001"
      ],
      "default": "gpt-4",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "temperature": {
      "type": "decimal",
      "default": 0.75,
      "max": 1,
      "min": 0
    },
    "max_tokens": {
      "type": "integer",
      "default": 250,
      "max": 500,
      "required": true
    },
    "top_p": {
      "type": "decimal",
      "default": 0.5,
      "max": 1,
      "min": 0
    },
    "frequency_penalty": {
      "type": "decimal",
      "default": 0,
      "max": 2,
      "min": 0
    },
    "presence_penalty": {
      "type": "decimal",
      "default": 0,
      "max": 2,
      "min": 0
    },
    "welcome_message": {
      "type": "text",
      "default": "Hello. How can I help you today?",
      "maxLength": 300,
      "minLength": 5
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3,
      "maxLength": 32
    },
    "chat_content_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-content.chat-content",
      "mappedBy": "bot_ids"
    },
    "chat_intention_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-intention.chat-intention",
      "mappedBy": "bot_ids"
    },
    "promotion_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::promotion.promotion",
      "mappedBy": "bot_ids"
    },
    "chat_user_info_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-user-info.chat-user-info",
      "mappedBy": "bot_ids"
    },
    "chat_style_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-style.chat-style",
      "inversedBy": "bot_ids"
    },
    "training": {
      "type": "text"
    },
    "memory": {
      "type": "integer",
      "default": 5,
      "required": true,
      "max": 10
    },
    "logit_bias": {
      "type": "json"
    },
    "chat_syntax_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat-syntax.chat-syntax",
      "inversedBy": "bot_ids"
    },
    "custom_triggers": {
      "type": "component",
      "repeatable": true,
      "component": "bots.trigger"
    },
    "general_training": {
      "type": "richtext"
    },
    "trigger_ids": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trigger.trigger",
      "mappedBy": "bot_ids"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "description": {
      "type": "text",
      "maxLength": 255
    }
  }
}
